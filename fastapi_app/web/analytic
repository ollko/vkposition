from fastapi import APIRouter, HTTPException, Depends
from fastapi_app.service import analytic as service
from fastapi_app.error import Missing, Duplicate
from .user import oauth2_dep
router = APIRouter(prefix="/analytic")


@router.get("/group")
def get_groups(token: str = Depends(oauth2_dep)) -> list[User]:
    return service.get_all()
